<app>
	<name>Example app</name>


	<simparams
		simtime="1:00:00"
		/>

	<user_defined>
		<agv uid="AGV_1" capacity="2"/>
	</user_defined>

	<entities>
		<source uid="Source_1" spawn_time="uniform(5, 15)" />
		<source uid="Source_Agv" spawn_entity="AGV_1" spawn_count="1" />

		<station uid="Station_0" operation_time="uniform(5, 12)" />

		<conveyor uid="Conveyor_1" length="5"  speed="1"/>

		<station uid="Station_1" operation_time="uniform(5, 13)" />
		<buffer uid="Buffer_1" limit="500" />

		<single_lane uid="L1" length="5"/>
		<single_lane uid="L2" length="2.5"/>
		<single_lane uid="L3" length="5"/>
		<single_lane uid="L4" length="2,5"/>

		<loader uid="Loader_1" loading="00:05"/>

		<unloader uid="Unloader_1"/>

		<buffer uid="Buffer_2" limit="500" />

		<station uid="Station_3" operation_time="uniform(5, 11)" />

		<drain uid="Drain_1" />
	</entities>

	<connections>
		<single a="Source_1" b="Station_0" />

		<single a="Station_0" b="Conveyor_1" />

		<single a="Conveyor_1" b="Station_1" />

		<single a="Conveyor_1" b="Station_1" />

		<single a="Station_1" b="Buffer_1" />

		<single a="Loader_1" b="Buffer_1" />

		<single a="Unloader_1" b="Buffer_2" />

		<single a="Buffer_2" b="Station_3" />

		<single a="Station_3" b="Drain_1" />
	</connections>


</app>